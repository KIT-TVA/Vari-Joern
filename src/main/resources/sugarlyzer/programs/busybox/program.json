{
  "remove_errors": false,
  "config_prefix": "KGENMACRO_",

  "make_target": "defconfig",
  "kconfig_root_file_path": "Config.in",
  "config_header_path": "include/autoconf.h",

  "included_files_and_directories": [
    {
      "macro_definitions": [
        "-D __extension__",
        "-D restrict",
        "-D __restrict",
        "-D _Pragma(x)",
        "-D ___builtin_offsetof(st,m)=((size_t)&(((st *)0)->m))",
        "-D ENABLE_LOCALE_SUPPORT=1",
        "-U FAST_FUNC"
      ]
    }
  ]
}